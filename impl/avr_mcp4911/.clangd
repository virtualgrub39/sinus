CompileFlags:
  Add:
    [
      "-g",
      "-Os",
      "-Wall",
      "-Wextra",
      "-fpermissive",
      "-fno-exceptions",
      "-fno-threadsafe-statics",
      "-ffunction-sections",
      "-fdata-sections",
      "-pipe",
      "-D__AVR__",
      "-D__AVR_ATtiny85__",
      "-std=gnu99",
      "-x", "c",
      "-target", "avr-none-gnu",
      "-mmcu=attiny85",
      "-DF_CPU=8000000UL",
      "-I../../",
    ]
  Compiler: avr-gcc
