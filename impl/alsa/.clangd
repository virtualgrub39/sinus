CompileFlags:
  Add:
    [
      "-g",
      "-Os",
      "-Wall",
      "-Wextra",
      "-fpermissive",
      "-fno-exceptions",
      "-fno-threadsafe-statics",
      "-ffunction-sections",
      "-fdata-sections",
      "-pipe",
      "-std=c99",
      "-x", "c",
      "-I../",
      "-Wc99-designator",
    ]
  Compiler: gcc
